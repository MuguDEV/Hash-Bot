{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "nixpacks",
    "buildCommand": "python3 -m pip intall -r requirements.txt"
  },
  "deploy": {
    "startCommand": "python3 bot.py",
    "healthcheckPath": "/",
    "healthcheckTimeout": 12,
    "restartPolicyType": "never"
  }
}
